#!name=Spotify(>=iOS15)
#!desc=部分解锁

[Mitm]
hostname =spclient.wg.spotify.com

[Script]
http-request ^https:\/\/spclient\.wg\.spotify\.com\/(artistview\/v1\/artist|album-entity-view\/v2\/album)\/ script-path=https://raw.githubusercontent.com/2023djash/blog-img/main/spotify-json.js, timeout=10, tag=spotifyJson
http-response ^https:\/\/spclient\.wg\.spotify\.com\/(bootstrap\/v1\/bootstrap|user-customization-service\/v1\/customize)$ script-path=https://raw.githubusercontent.com/2023djash/blog-img/main/spotify-proto.js, requires-body=true, binary-body-mode=true, timeout=10, tag=spotifyProto
